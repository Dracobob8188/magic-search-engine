.printings_box
  - printings[0,15].each do |set_name, set_printings|
    - if set_printings.size == 1
      - set_printings.each do |type, printing|
        - case type
        - when :selected
          .selected
            = printing.set_name
        - when :matching
          .matching
            = link_to_card(printing) do
              = printing.set_name
        - else
          .not_matching
            = link_to_card(printing) do
              = printing.set_name
    - else
      %div
        %span.set_name
          = set_name
          &mdash;
        - set_printings.each do |type, printing|
          - case type
          - when :selected
            %span.selected
              = printing.number
          - when :matching
            %span.matching
              = link_to_card(printing) do
                = printing.number
          - else
            %span.not_matching
              = link_to_card(printing) do
                = printing.number

  - if printings.size > 15
    = link_to_card(card) do
      (and others)
